<!DOCTYPE html>
<html lang="zxx" class="no-js">

<head>
    <!-- Mobile Specific Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Favicon-->
    <link rel="shortcut icon" href="~/img/fav.png">
    <!-- Author Meta -->
    <meta name="author" content="CodePixar">
    <!-- Meta Description -->
    <meta name="description" content="">
    <!-- Meta Keyword -->
    <meta name="keywords" content="">
    <!-- meta character set -->
    <meta charset="UTF-8">
    <!-- Site Title -->
    <title>Pets Shop</title>

    <!--
            CSS
            ============================================= -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">
    <link href="https://fonts.googleapis.com/css?family=Mulish:300,400,500,600,700,800,900" rel="stylesheet" type="text/css" media="all">
    <link rel="stylesheet" href="~/Content/linearicons.css">
    <link rel="stylesheet" href="~/Content/owl.carousel.css">
    <link rel="stylesheet" href="~/Content/themify-icons.css">
    <link rel="stylesheet" href="~/Content/nice-select.css">
    <link rel="stylesheet" href="~/Content/nouislider.min.css">
    <link rel="stylesheet" href="~/Content/bootstrap.css">
    <link rel="stylesheet" href="~/Content/main.css">
    @*thư viện chạy ajax*@
    <script src="~/Scripts/Admin/jquery.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
</head>

<body>

    <!-- Start Header Area -->
    @Html.Action("MenuPartial", "Home")
    <!-- End Header Area -->
    <!-- Start Banner Area -->
    <section class="banner-area organic-breadcrumb">
        <div class="container">
            <div class="breadcrumb-banner d-flex flex-wrap align-items-center justify-content-end">
                <div class="col-first">
                    <h1>@PetsProject.Resources.Data.ShoppingCartTitle</h1>
                    <nav class="d-flex align-items-center">
                        <a href="index.html">@PetsProject.Resources.Data.Home<span class="lnr lnr-arrow-right"></span></a>
                        <a href="category.html">@PetsProject.Resources.Data.Cart</a>
                    </nav>
                </div>
            </div>
        </div>
    </section>
    <!-- End Banner Area -->
    <!--================Cart Area =================-->
    <section class="cart_area">
        <div class="container">
            <div class="cart_inner">

                @RenderBody()


            </div>
        </div>
    </section>
    <!--================End Cart Area =================-->
    <!-- start footer Area -->
    <footer>
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-3">
                    <div class="logo_footer">
                        <a href="@Url.Action("Index", "Home")">
                            <img src="~/img/icon/logo.png" />
                        </a>
                    </div>
                </div>
                <div class="col-md-3">
                    <h2>Menu</h2>
                    <ul>
                        <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                        <li><a href="@Url.Action("SanPham", "SanPham")">Product</a></li>
                        <li><a href="@Url.Action("About", "Home")">About</a></li>
                        <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
                    </ul>
                </div>
                <div class="col-md-3">
                    <h2>Contact</h2>
                    <ul>
                        <li><a href="@Url.Action("Contact", "Home")">principal office: Hamlet 6, Linh Trung Ward, Thu Duc District, Ho Chi Minh City,</a></li>
                        <li><a href="#">phone: 1800 1211</a></li>
                        <li><a href="#">email: gallmount1899@gmail.com</a></li>
                    </ul>
                </div>
                <div class="col-md-3">
                    <h2>connect to us</h2>
                    <p class="social">
                        <a href="https://www.facebook.com/nguyenson.nguyen.12576"><i class="fa fa-facebook-square"></i></a>
                        <a href="#"><i class="fa fa-twitter-square"></i></a>
                        <a href="#"><i class="fa fa-google-plus-square"></i></a>
                        <a href="#"><i class="fa fa-linkedin-square"></i></a>
                    </p>
                </div>
                <hr>
                <div class="col-md-12 text-center">
                    <p>Group 9 .NET. All Rights Reserved</p>
                </div>
            </div>
        </div>
    </footer>
    <!-- End footer Area -->


    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4"
            crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-migrate-3.0.0.min.js"></script>
    <script src="~/Scripts/vendor/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.ajaxchimp.min.js"></script>
    <script src="~/Scripts/jquery.nice-select.min.js"></script>
    <script src="~/Scripts/jquery.sticky.js"></script>
    <script src="~/Scripts/nouislider.min.js"></script>
    <script src="~/Scripts/jquery.magnific-popup.min.js"></script>
    <script src="~/Scripts/owl.carousel.min.js"></script>
    <!--gmaps Js-->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjCGmQ0Uq4exrzdcL6rvxywDDOvfAu6eE"></script>
    <script src="~/Scripts/gmaps.min.js"></script>
    <script src="~/Scripts/main.js"></script>
</body>

</html>