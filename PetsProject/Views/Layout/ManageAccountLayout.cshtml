        function saveProcess() {
            // Kiem tra mat khau
            var newPass = $('#newpass').val();
            var rePass = $('#confirmpass').val();
            if (newPass != rePass) {
                alert("The password not match");
                return false;
            } if (newPass.length < 8) {
                alert("The password has at least 8 characters. Try again");
                return false;
            }

            savePassword();
            return false;
        }



